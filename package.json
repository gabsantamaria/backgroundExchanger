{
  "name": "backgroundExchanger",
  "version": "1.0.0",
  "description": "electronjs app",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "install": "electron-rebuild",
    "package-mac": "electron-packager . backgroundExchanger --overwrite --platform=darwin --arch=x64 --prune=false",
    "package-win-32": "electron-packager . backgroundExchanger --overwrite --platform=win32 --arch=ia32 --asar=true",
    "package-win-64": "electron-packager . backgroundExchanger --overwrite  --platform=win32 --arch=x64 --prune=false",
    "package-linux": "electron-packager . backgroundExchanger --overwrite  --platform=linux --arch=x64 --prune=false"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/palmero03/backgroundExchanger.git"
  },
  "keywords": [
    "Electron",
    "serial",
    "port"
  ],
  "author": "DPalmero",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.4.1",
    "electron-rebuild": "^1.4.0"
  },
  "dependencies": {
    "data-table": "^2.0.0",
    "git": "^0.1.5",
    "serialport": "^4.0.7",
    "ws": "^5.1.1"
  }
}
